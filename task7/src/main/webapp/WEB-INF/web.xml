<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>nameServlet</servlet-name>
        <servlet-class>NameServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>nameServlet</servlet-name>
        <url-pattern>/get</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>pageServlet</servlet-name>
        <servlet-class>PageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>pageServlet</servlet-name>
        <url-pattern>/page</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>postServlet</servlet-name>
        <servlet-class>PostServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>postServlet</servlet-name>
        <url-pattern>/check</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>statusServlet</servlet-name>
        <servlet-class>StatusServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>statusServlet</servlet-name>
        <url-pattern>/status</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>test1Servlet</servlet-name>
        <servlet-class>Test1Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>test1Servlet</servlet-name>
        <url-pattern>/test1</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>test2Servlet</servlet-name>
        <servlet-class>Test2Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>test2Servlet</servlet-name>
        <url-pattern>/test2</url-pattern>
    </servlet-mapping>

    <filter>
        <filter-name>filter</filter-name>
        <filter-class>MyFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>filter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
</web-app>